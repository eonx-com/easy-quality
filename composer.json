{
    "name": "eonx-com/easy-quality",
    "description": "Makes using of code quality tools for PHP projects of EonX easier",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "jangregor/phpstan-prophecy": "^0.8.0",
        "phpmd/phpmd": "2.8.*",
        "phpstan/phpdoc-parser": "^0.4",
        "phpstan/phpstan": "0.12.74",
        "rector/rector": "^0.8",
        "slevomat/coding-standard": "^6.4",
        "symfony/dependency-injection": "5.1.10",
        "symplify/easy-coding-standard": "^8.3",
        "symplify/phpstan-extensions": "^8.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symplify/easy-coding-standard-tester": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "EonX\\EasyQuality\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EonX\\EasyQuality\\Tests\\": "tests"
        }
    }
}
